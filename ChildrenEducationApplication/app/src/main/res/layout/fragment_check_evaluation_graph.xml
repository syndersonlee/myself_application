<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="#FFFFFF"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:clipToPadding="false"
            android:id="@+id/rv1wt319lfwn"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#E3E3E3"
            android:paddingTop="58dp"
            android:paddingHorizontal="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/r7lnqyj4mce"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="23dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/graph_back_button"
                        android:layout_width="35dp"
                        android:layout_height="40dp"
                        android:layout_marginEnd="18dp"
                        android:src="@drawable/back_icon"
                        android:clickable="true"
                        android:focusable="true"
                        android:scaleType="fitXY"
                        />
                    <ImageView
                        android:id="@+id/r3r07dlxq8zn"
                        android:layout_width="65dp"
                        android:layout_height="60dp"
                        android:layout_marginEnd="2dp"
                        android:src="@drawable/bar_graph"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/rzt1rhrcsq0d"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="나의 성장곡선 확인하기"
                        android:textStyle="bold"
                        />
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/rt2et4z7vb0f"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/cr20b00ca51"
                    android:paddingVertical="11dp"
                    android:layout_marginBottom="22dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/rjjbkyhsrgjk"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="나의 성장 곡선"
                        android:textStyle="bold"

                        />
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/rkxfh98na2g"
                    android:layout_width="match_parent"
                    android:layout_height="207dp"
                    android:background="@drawable/cr20bffffff"
                    android:layout_marginBottom="21dp"
                    android:orientation="vertical">
                    <com.github.mikephil.charting.charts.BarChart
                        android:id="@+id/bar_chart_growth"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="10dp"
                        android:layout_marginHorizontal="10dp"/>
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/ry9x6i3a8azl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/cr20b56a3ff"
                    android:paddingVertical="13dp"
                    android:layout_marginBottom="21dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/r73uyz9zejxt"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="점검 정확도 성장곡선"
                        android:textStyle="bold"
                        />
                </LinearLayout>
                <LinearLayout
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/rmmlco40f7eb"
                    android:layout_width="match_parent"
                    android:layout_height="207dp"
                    android:background="@drawable/cr20bffffff"
                    android:orientation="vertical">
                    <com.github.mikephil.charting.charts.BarChart
                        android:id="@+id/bar_chart_accuracy"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginVertical="10dp"
                        android:layout_marginHorizontal="10dp"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</LinearLayout>