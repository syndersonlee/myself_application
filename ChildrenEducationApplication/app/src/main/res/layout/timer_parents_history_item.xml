<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="7dp"
    android:background="@drawable/cr20bffffff"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:paddingHorizontal="25dp"
    android:layout_marginBottom="5dp">

    <TextView
        android:id="@+id/timer_history_create_time"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <LinearLayout
        android:id="@+id/children_check"
        android:layout_width="72dp"
        android:layout_height="74dp"
        android:background="#E3E3E3"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/children_check_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/yes_checkbox" />
    </LinearLayout>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <LinearLayout
        android:id="@+id/parents_check"
        android:layout_width="72dp"
        android:layout_height="74dp"
        android:background="#E3E3E3"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/parents_check_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/no_checkbox" />
    </LinearLayout>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1" />

    <TextView
        android:id="@+id/rxi2q6ssc2de"
        android:layout_width="58dp"
        android:layout_height="56dp"
        android:gravity="center"
        android:text="불일치"
        android:textSize="15sp"
        android:textStyle="bold" />
</LinearLayout>